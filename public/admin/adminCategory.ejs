{%- include('partials/header') -%}

<h1>admin category</h1>
<h6>total records: {%= data?.length %}</h6>
<div class="d-flex m-3 justify-content-evenly flex-wrap">

    



                {% data.map( (item)=> { %}
                    <div class="card d-flex gap-2 m-3 " style="width: 18rem">

                        <div class="card-body p-2   ">
                            <h5 class="card-title">Title: {%= item?.title %}</h5>
                            <h5 class="card-title">approvedByAdmin: {%= item?.approvedByAdmin %}</h5>
                            <h5 class="card-title">createdAt: {%= item?.createdAt %}</h5>
                            <h5 class="card-title">updatedAt: {%= item?.updatedAt %}</h5>
                            <a href="#" class="btn btn-success">Approve</a>
                            <a href="#" class="btn btn-danger">Reject</a>
                        </div>

                    </div>
                {% } ); %}


    
</div>

<div class="text-center">
    <a href="/admin" class="btn btn-primary">Go to AdminPanel</a>
</div>

<p>data: {%= data %}</p>

{%- include('partials/footer') -%}
