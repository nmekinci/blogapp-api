{%- include('partials/header') -%}
<h1>admin Report</h1>
<h6>total records: {%= data?.length %}</h6>

<div class="d-flex m-3 justify-content-evenly flex-wrap">

    {% data.map( (item)=> { %}
        <div class="card d-flex gap-2 m-3 " style="width: 18rem">

            <div class="card-body p-2">
                <p class="card-text">Id: {%= item?._id %}</p>
                <p class="card-title">userId: {%= item?.userId?._id %}</p>
                <p class="card-title">userName: {%= item?.userId?.userName %}</p>
                <p class="card-title">blogId: {%= item?.blogId?._id %}</p>
                <p class="card-title">blogTitle: {%= item?.blogId?.title %}</p>
                <p class="card-title">blogCategoryId: {%= item?.blogId?.category?._id %}</p>
                <p class="card-title">blogCategoryTitle: {%= item?.blogId?.category?.title %}</p>
                <p class="card-title">Report: {%= item?.report %}</p>
                <a href="#" class="btn btn-success">Approve</a>
                <a href="#" class="btn btn-danger">Reject</a>
            </div>

        </div>
    {% } ); %}

</div>

<div class="text-center">
    <a href="/admin" class="btn btn-primary">Go to AdminPanel</a>
</div>

<p>data: {%= data %}</p>

{%- include('partials/footer') -%}